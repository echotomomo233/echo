# to achieve a function 'is_prime'ï¼što judge if a positive integer is prime number
# input:a positive integer 'n'
# output:True or False

def is_prime(n): 
    for a in range(2,n):
        if n % a == 0:
            return False
        return True

    if n == 1:
        return False
    if n == 2:
        return True

n = int(input())
print(is_prime(n))
